extends layout

block content
    h1= title
    ul
        //#{flights}
        //console.log(flights)
        //=JSON.stringify(flights[13].getInformation())
        each flight, index in flights
            = index
            if typeof(flight) !== 'undefined' 
                - flight = flight.getInformation()
                li= flight.number + ": " + flight.origin + "-" + flight.destination
            